<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="textView">
        <item name="android:textAppearance">?textAppearanceBody1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="button">
        <item name="android:textAppearance">?textAppearanceBody1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="OutlinedBoxTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">@color/gray</item>
    </style>

       <style name="FilleddBoxTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="boxStrokeColor">@color/gray</item>
    </style>




